{
    "routes": [
      {
        "route": "/page1.html",
        "allowedRoles": ["authenticated"]
      },
      {
        "route": "/page2.html",
        "allowedRoles": ["authenticated"]
      }
    ],
    "responseOverrides": {
      "401": {
        "statusCode": 302,
        "redirect": "/.auth/login/aadb2c"
      }
    },
    "auth": {
      "identityProviders": {
        "customOpenIdConnectProviders": {
          "aadb2c": {
            "registration": {
              "clientIdSettingName": "AADB2C_PROVIDER_CLIENT_ID",
              "clientCredential": {
                "clientSecretSettingName": "AADB2C_PROVIDER_CLIENT_SECRET"
              },
              "openIdConnectConfiguration": {
                "wellKnownOpenIdConfiguration": "https://fashionappdemo.b2clogin.com/fashionappdemo.onmicrosoft.com/B2C_1_susi/v2.0/.well-known/openid-configuration"
              }
            },
            "login": {
              "nameClaimType": "Email Addresses",
              "scopes": ["openid"]
            }
          }
        }
      }
    }
  }